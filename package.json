{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for devtinder",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "type-check": "tsc --noEmit",
    "seed-users": "node src/scripts/runSeedUsers.js",
    "count-users": "node src/scripts/runCountUsers.js"
  },
  "author": "Soumen Mondal",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  }
}
